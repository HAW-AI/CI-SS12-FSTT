module m10;

const
 sieben = 3+4;

type
 vektor = array[3] of integer;
 
var
 i  : integer;
 x  : vektor;
 
procedure factorial(j : integer);

 begin
  if j = 1
  then
  else i := i * j; factorial(j - 1)
  end
 end factorial;

begin
 i := 1;
 factorial(sieben);
 x[0] := 1*1+sieben;
 x[1] := 3;
 x[2] := x[0]+1;
 Print x[0];
 Print x[1];
 Print x[2];
 Print i;
end m10.   
